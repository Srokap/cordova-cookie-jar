	id="is.assembly.cordova-cookie-jar"
	version="1.0.0">
	<name>Cordova Cookie Jar</name>
	<asset src="www/cordova-cookie-jar.js" target="cordova-cookie-jar.js" />
	<platform name="ios">
		<plugins-plist key="CDVCookieJar" string="CDVCookieJar" />
